
<%@categories.each do |category|%>
    <p class= "categories"><%=category.name%> </P>
    <ul>
    <div class= "meme_container">
    <%category.memes.each do |meme|%>
        <div class= "meme_img_owner"> 
        <div class ="meme_img_container"><%= link_to image_tag((meme.source),:class => "meme_img") ,meme_path(meme)%></div>
        <li class ="meme_owner"> created by: <span class="owner_name"><%=meme.owner.username%></span> </li>
        </div>
    <%end%>
    </div>
    </ul>
<%end%>
